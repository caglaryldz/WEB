@model List<UserModel>
@{
    ViewData["Title"] = "Users";
}
<div class="row">
    <div class="col">
        <h1 class="display-4"> Users</h1>
    </div>
</div>

<div class="row">
    <div class="col">
        <table class="table table-hover table-sm">
            <thead>
                <tr>
                    <th>KullanıcıAdıSoyadı</th>
                    <th>KullanıcıAdı</th>
                    <th>Locked</th>
                    <th>Role</th>
                    <th>Created A</th>
                    <th></th>

                </tr>
            </thead>
            <tbody>
                @foreach (UserModel user in Model)
                {
                    <tr>
                        <th>@user.FullName</th>
                        <th>@user.Username</th>
                        <th>@user.Locked</th>
                        <th>@user.Role</th>
                        <th>@user.CreatedAt</th>
                        <th>
                            <button type="button" class="btn btn-info btn-sm"><i class="fa fa-edit me-2">Edit</i></button>
                            <button type="button" class="btn btn-danger btn-sm"><i class="fa fa-trash me-2">Edit</i></button>

                        </th>

                    </tr>

                }
            </tbody>

        </table>

    </div>

</div>